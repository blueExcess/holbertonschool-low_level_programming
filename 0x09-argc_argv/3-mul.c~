/* 3. function that multiplies two numbers */
#include "holberton.h"
#include <stdio.h>

/**
 * main - multiply two numbers
 * @argc: number of args
 * @argv: args
 *
 * Return: 0 on success, 1 if passed more than 2 args
 */
int main(int argc, char **argv)
{
	int x = argv[1], y = argv[2], value;

	/* need to convert x and y from char to int using atoi() */
	value = x * y;

	return (value);
}
